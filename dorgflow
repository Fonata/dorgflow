#!/usr/bin/env php
<?php

// Just test stuff for now.
namespace Dorgflow;

print "Hello, this is Dorgflow!\n";

require_once __DIR__ . '/vendor/autoload.php';

$command_runner = new CommandBuilder();
$command = $command_runner->getCommand($argv);

try {
  $command->execute();
}
catch (\Exception $e) {
  print $e->getMessage() . "\n";
}
